{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "SSRSImportExportWizardTest/SSRSImportExportWizardTest.csproj",
            "SSRSImportExportWizard/SSRSImportExportWizard/SSRSImportExportWizard.csproj",
            "SSRSImportExportConsole/SSRSImportExportConsole.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ],
          "src": ".."
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml",
      "properties": {
        "TargetFramework": "net452"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/*.yml",
          "**/index.md",
          "toc.yml"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "wwwroot/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "wwwroot/**"
        ]
      },
      {
        "files": [
          "**/Snapshots/**"
        ],
        "src": "../SSRSImportExportWizardTest",
        "dest": "images"
      },
      {
        "files": [
          "resources/**"
        ],
        "exclude": [
          "obj/**",
          "wwwroot/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "./**.md"
        ],
        "exclude": [
          "obj/**",
          "wwwroot/**"
        ]
      }
    ],
    "dest": "wwwroot",
    "globalMetadata": {
      "_appTitle": "SSRS Import Export Wizard",
      "_appFooter": "Copyright © DXC 2019",
      "_appLogoPath": "resources/dxc.logo.png",
      "_appFaviconPath": "resources/dxc.favicon.png",
      "_disableBreadcrumb": true,
      "_disableAffix": true,
      "_disableContribution": true
    }
  },
  "pdf": {
    "content": [
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md",
          "pdf/*"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "wwwpdf/**",
          "**/toc.yml",
          "**/toc.md"
        ]
      },
      {
        "files": "pdf/toc.yml"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "wwwpdf/**"
        ]
      },
      {
        "files": [
          "**/Snapshots/**"
        ],
        "src": "../SSRSImportExportWizardTest",
        "dest": "images"
      }
    ],
    "overwrite": [
      {
        "files": [
          "api/**.md",
          "./**.pdf"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "wwwpdf/**"
        ]
      }
    ],
    "dest": "wwwpdf"
  }
}