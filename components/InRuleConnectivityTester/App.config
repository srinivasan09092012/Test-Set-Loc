<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <appSettings>
    <add key="InRule.ProcessingMode" value="Remote" />
    <add key="InRule.ServiceUrl" value="http://bre.dev.mapshc.com" />
    <add key="InRule.UserName" value="BASConsumer" />
    <add key="InRule.Password" value="Password2015" />
    <add key="InRule.ApplicationId" value="BREApiTestApplication" />
    <add key="TenantId" value="081E354B-2184-47FE-B69D-3C5229D8BCCF" />
  </appSettings>
  <log4net xmlns="urn:log4net">
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="Logs\AppLog.txt" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="5" />
      <maximumFileSize value="10MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%utcdate{ABSOLUTE} [%thread] %level - %message%newline" />
      </layout>
      <threshold value="ALL" />
    </appender>
    <root>
      <appender-ref ref="RollingFileAppender" />
    </root>
  </log4net>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-10.0.0.0" newVersion="10.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
